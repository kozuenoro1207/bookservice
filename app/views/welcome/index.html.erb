<div class="box">
<% if logged_in? %>
  <div class="row">
    <aside class=""></aside>
    <div class="feed_talkrooms">
      <h1>質問一覧</h1>
      
      <% if @feed_items.any? %>
          <ol class="talkrooms">
            <%= render @feed_items %>
          </ol>
      <% end %>
    </div>
   </div>
<% else %>
  <div class="jumbotron">
      <div class="container-fluid">
        <h1>ぶっくぼっくす</h1>
        <p>新しい本に出会ってみよう</p>
        <div class="col-lg-2 col-lg-offset-5">
          <%= link_to "会員登録", signup_path, class: "btn btn-lg btn-warning" %>
        </div>
      </div>
    </header>
<% end %>
</div>