<h1></h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@talkroom) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.text_field :talkname , class: "form-control", placeholder: "どんな本がよみたい？" %>
      <%= f.submit "おすすめをきいてみる", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>

